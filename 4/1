x=98
y=22
if (x>y):
    print("en buyuk sayı:" ,x)
    #<eğer koşul doğru(true) ise çalışacak kodlar>
elif(x==y):
    print("sayılar eşittir")
else:
    print("en buyuk sayı:",y)
    #<eğer koşul yanlış(false) ise çalışacak kodlar>

ad = 'akça'
şifre = 123
if (ad=='azra' and şifre==123):
    print("griş başarılı")
elif(ad=='azra' and şifre!=123):
    print("şifre hatalı")
else:
    (ad!='azra' and şifre==123)
    print("ad hatalı")

