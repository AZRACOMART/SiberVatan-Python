dict = {'a@mail.com':'123','b@mail.com':'456'}
input_mail=input("mail giriniz:")
input_şifre=input("şifre giriniz:")
sonuc_mail=(input_mail in dict.keys())
sonuc_şifre=(input_şifre in dict.values())
sonuc_final=(int(sonuc_mail)+int(sonuc_şifre)==2)
print(sonuc_final)
 
 
