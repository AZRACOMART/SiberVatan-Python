tercih_input=int(input("hangi işlemi tercih edersiniz?"))
sayı_input=int(input("ilk sayıyı gir:"))
sayı2_input=int(input("ikinci sayıyı gir:"))

if tercih_input==1:
    print(sayı_input+sayı2_input)
elif tercih_input==2:
    print(sayı_input-sayı2_input)
elif tercih_input==3:
    print(sayı_input*sayı2_input)
else: 
    print(sayı_input/sayı2_input) 
